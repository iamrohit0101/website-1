{
    "host": "YOUR_HOST",
    "port": 80,
    "public": "../public/",
    "key": "YOUR_PRIVKEY_SSL_FILE",
    "cert": "YOUR_CERT_SSL_FILE",
    "ca": "YOUR_CHAIN_SSL_FILE",
    "paginate": {
      "default": 10,
      "max": 50
    },
    "authentication": {
      "secret": "token",
      "strategies": [
        "jwt",
        "local",
        "local-username"
      ],
      "path": "/authentication",
      "entity": "user",
      "service": "users",
      "passReqToCallback": true,
      "session": false,
      "cookie": {
        "enabled": false,
        "name": "feathers-jwt",
        "httpOnly": false,
        "secure": false
      },
      "jwt": {
        "header": {
          "type": "access"
        },
        "audience": "YOUR_HOST",
        "subject": "anonymous",
        "issuer": "feathers",
        "algorithm": "HS256",
        "expiresIn": "7d"
      },
      "local": {
        "entity": "user",
        "service": "users",
        "usernameField": "email",
        "passwordField": "password"
      },
      "patreon": {
        "clientID": "YOUR_PATREON_CLIENT_ID",
        "clientSecret": "YOUR_PATREON_CLIENT_SECRET",
        "campaignID": "YOUR_CAMPAIGN_ID"
      }
    },
    "skipIPs": ["SKIP_IPS"],
    "adminIPs": ["ADMIN_IPS"],
    "mongodb": "MONGODB_URI",
    "awsAccessKey": "YOUR_AWS_ACCESS_KEY",
    "awsSecretKey": "YOUR_AWS_SECRET_KEY"
  }
  
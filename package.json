{
  "name": "angelthump",
  "description": "Live Streaming Service",
  "version": "4.1.0",
  "homepage": "https://angelthump.com",
  "main": "src/",
  "keywords": [
    "live streaming"
  ],
  "license": "MIT",
  "repository": {},
  "author": {
    "name": "",
    "email": ""
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "dev": "nodemon --watch src/ --watch config/ src/index.js",
    "build-dashboard": "browserify public/react/dashboard.jsx -g browserify-css -t [ babelify --presets [  @babel/preset-env @babel/preset-react ] ] -t uglifyify | uglifyjs -cm > public/js/dashboard.js",
    "build-admin": "browserify public/react/admin.js -g browserify-css -t [ babelify --presets [  @babel/preset-env @babel/preset-react ] ] -t uglifyify | uglifyjs -cm > public/js/admin.js",
    "build-dev": "browserify public/react/admin.js -t [ babelify --presets [  @babel/preset-env @babel/preset-react ] ] -o public/js/admin.js",
    "watch": "watchify public/react/dashboard.jsx -v --extension=.jsx -t [ babelify --presets [  @babel/preset-env @babel/preset-react ] ] -o public/js/dashboard.js",
    "test": "snyk test && npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "mocha": "mocha test/ --recursive"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.3.10",
    "@feathersjs/authentication-client": "^4.3.10",
    "@feathersjs/authentication-local": "^4.3.10",
    "@feathersjs/authentication-oauth": "^4.3.10",
    "@feathersjs/client": "^4.3.10",
    "@feathersjs/configuration": "^4.3.10",
    "@feathersjs/errors": "^4.3.10",
    "@feathersjs/express": "^4.3.10",
    "@feathersjs/feathers": "^4.3.10",
    "@feathersjs/socketio": "^4.3.10",
    "@feathersjs/socketio-client": "^4.3.10",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "css-modulesify": "^0.28.0",
    "ejs": "^2.7.1",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "ra-data-simple-rest": "^2.9.8",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "serve-favicon": "^2.5.0",
    "uglify-es": "^3.3.9",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "browserify-css": "^0.15.0",
    "eslint": "^6.6.0",
    "jshint": "^2.10.3",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  }
}
